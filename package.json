{
  "name": "xcpc-tools-workspace",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:client": "node -r @hydrooj/utils/lib/register packages/client/index.ts",
    "start:server": "node -r @hydrooj/utils/lib/register packages/server/index.ts",
    "build": "node -r @hydrooj/utils/lib/register build.ts && pkg dist/entry.js"
  },
  "devDependencies": {
    "@hydrooj/eslint-config": "^1.0.7",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "pkg": "^5.8.1",
    "typescript": "^5.3.3"
  }
}
